public class A01{
	public static void main (String[]args){
		double [] arr={1.1,1.5,1.5,2.8,1,1.9};
		MAX a=new  MAX();
		System.out.println(a.homework(arr));
	}
}
class MAX{
	public double homework(double [] arr){
		double j=arr[0];
		for(int i=0;i<arr.length;i++){
			if(j<arr[i]){
				 j=arr[i];
			}
		}
		return j;
	}
}
