import java.util.Scanner;
public class today{
    public static void main(String []args) {
      Scanner my = new Scanner(System.in);
 	  char week = my.next().charAt(0);
	  switch (week)
	  {
		  case 'a':
			  System.out.println("星期一");
			  break;
		  case 'b':
			  System.out.println("星期二");
			  break;
	       default :
			  ..........;
			  break;
    }
}
