import java.util.Scanner;
public class contructor 
{
    public static void main(String []args) 
   {
   	HspMethods a =new HspMethods("momo",15);
   	
   	System.out.println(a.name +" "+ a.age);
 	HspMethods a1 =new HspMethods();
 	 
 	 System.out.println(a1.age );
   }
}
class HspMethods{
	String name;
	int age;
	public  HspMethods (String name1,int age1)
	{
		 age =age1 ;
		 name = name1;
	}
	HspMethods(){
		age =18;
	}
	
}
