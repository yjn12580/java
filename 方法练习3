import java.util.Scanner;
public class swtich{
    public static void main(String []args) {
    	Person p=new Person();
    	p.name="丁真";
    	p.age=10;
    	AB tool=new AB();
    	Person p2=tool.copyperson(p);
    	}
}
class Person
{
	String name;
	int age;
}
class AB
{
	public Person copyperson (Person p)
	{
		Person p2=new Person();
		p2.name=p.name;
		p.age=p2.age;
		return p2;
	}
}
